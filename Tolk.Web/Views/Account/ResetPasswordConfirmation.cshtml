@{
    ViewData["Title"] = "Ange nytt lösenord";
}
<section aria-label="@ViewData["Title"]">
    <h1 class="padding-bottom-30px">@ViewData["Title"]</h1>
    <div class="row">
        <div class="col-sm-12 text-align-center">
            <h2 class="padding-bottom-30px">Lösenord återställt <span class="glyphicon glyphicon-ok"></span></h2>
