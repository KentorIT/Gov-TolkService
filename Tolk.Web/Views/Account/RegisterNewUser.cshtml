@model RegisterNewUserViewModel

@{
    ViewData["Title"] = "RegisterNewUser";
}

<h2>RegisterNewUser</h2>

<div class="row">
    <div class="col-md-4">
        <form method="post" asp-action="RegisterNewUser" asp-controller="Account">
            <form-entry asp-for="Password" />
            <form-entry asp-for="PasswordRetype" />
            <form-entry asp-for="NameFirst" />
            <form-entry asp-for="NameFamily" />
            <form-entry asp-for="PhoneWork" />
            <form-entry asp-for="PhoneCellphone" />
            <button type="submit" class="btn btn-success">Skapa</button>
        </form>
    </div>
</div>

@section Scripts{
    @await Html.PartialAsync("_ValidationScriptsPartial")
}
