@model IEnumerable<NotificationSettingsModel>
@{
    ViewData["Title"] = "Organisationens notifieringsinställningar";
}
<h2>@ViewData["Title"]</h2>
<form method="post">
    @foreach (var row in Model)
    {
        @await Html.PartialAsync("_EditNotificationSettingsRow", row)
    }
    <div class="row">
        <div class="col-sm-4">
            <a class="btn btn-large btn-warning pull-left do-not-check-dirty" asp-controller="Home" asp-action="Index">Avbryt <span class="btn-remove-times">&times;</span></a>
        </div>
        <div class="col-sm-4">
            <button type="submit" class="btn btn-large btn-success pull-right">Spara</button>
        </div>
    </div>

</form>
@section Scripts{
    @await Html.PartialAsync("_ValidationScriptsPartial")
}
