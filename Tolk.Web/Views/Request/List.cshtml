@model RequestListModel
@using Tolk.BusinessLogic.Utilities
@inject HelpLinkService HelpLinkService
@using Tolk.Web.Services
@{
    ViewData["Title"] = "Historik - Bokningar";
    HelpLinkService.PageName = "formedling-historik/";

}
    <section aria-label="@ViewData["Title"]">
        <h1>@ViewData["Title"] @HelpLinkService.HeaderAnchor("historik-forfragningar")</h1>
        <div class="row filter-panel">
            @await Html.PartialAsync("_Filter", Model.FilterModel)
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="table-responsive ajax-listing table-datatable">
                    <table class="table table-hover full-row-height clickable-rows-with-action search-table" data-order="[[ 1, &quot;desc&quot; ]]"
                           data-ajax-path="@Url.Action(nameof(Tolk.Web.Controllers.RequestController.ListRequests), "Request")"
                           data-ajax-column-definition="@Url.Action(nameof(Tolk.Web.Controllers.RequestController.ListColumnDefinition), "Request")"
                           data-click-action-url="@Url.Action(nameof(Tolk.Web.Controllers.RequestController.View), "Request")"
                           data-filter-selector=".filter-panel">
                    </table>
                </div>
            </div>
        </div>
    </section>