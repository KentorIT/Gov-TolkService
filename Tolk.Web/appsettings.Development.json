{
  "ConnectionStrings": {
    "DBConnection": "Server=.;Database=TolkDev;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Database": {
    "AutoMigrateOnStartup": true
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "PublicOrigin": "https://localhost:3140",
  "SupportEmail": "none@example.com",

  "Smtp": {
    "FromAddress": "noreply@example.com"
  },
  "EnableTimeTravel": "true",
  "SideBar": [
    {
      "Title": "Utveckling",
      "Message": "Det här är en utvecklingsmiljö, som innehåller senaste versionen av koden. Funktioner kan ändras snabbt och kan ibland vara halvfärdiga."
    },
    {
      "Title": "Ett till meddelande",
      "Message": "Man kan lägga till flera rutor om man vill."
    }
  ],
  "MonthsToApproveComplaints": "3",
  "HoursToApproveChangeInterpreterRequests": "2",
  "RoundPriceDecimals": true,
  "UserDocumentation": {
    "BaseUrl": "https://tolkdevdocs.azurewebsites.net/"
  },
  "Env": {
    "Name": "Development Local",
    "Background": "background: rgba(255, 255, 0, 0.5)",
    "Foreground": "color: darkgrey"
  }
}
  
